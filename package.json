{
	"name": "university-library",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
		"flask-migrate": "python3 -m flask --app api/index db migrate",
		"flask-upgrade": "python3 -m flask --app api/index db upgrade",
		"generate:types": "npx openapi-typescript http://localhost:5328/docs/openapi.json -o src/api/types.ts",
		"next-dev": "next dev --turbopack",
		"dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@hookform/resolvers": "^4.1.3",
		"@radix-ui/react-avatar": "^1.1.3",
		"@radix-ui/react-label": "^2.1.2",
		"@radix-ui/react-slot": "^1.1.2",
		"bcryptjs": "^3.0.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"concurrently": "^9.1.2",
		"lucide-react": "^0.479.0",
		"next": "15.2.3",
		"next-auth": "5.0.0-beta.25",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.54.2",
		"tailwind-merge": "^3.0.2",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"camelcase-keys": "^9.1.3",
		"eslint": "^9",
		"eslint-config-next": "15.2.1",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"openapi-typescript": "^7.6.1",
		"prettier": "^3.5.3",
		"tailwindcss": "^4",
		"type-fest": "^4.37.0",
		"typescript": "^5"
	}
}
